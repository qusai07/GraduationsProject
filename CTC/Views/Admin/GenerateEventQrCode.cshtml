@{
    ViewData["Title"] = "Generate QR Code";
    Layout = "~/Views/Shared/_LayoutDahsBoard.cshtml";

}
<form>
    <main id="main" class="main">
        <div class="pagetitle">
            <h4>Your QR Code:</h4>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a>Pages</a></li>
                    <li class="breadcrumb-item">QR Code</li>
                </ol>
            </nav>
        </div><!-- End Page Title -->

        <section class="section">
            <div class="row">
                <div class="col-lg-12">

                    <div class="container">

                        <div class="row d-flex justify-content-center align-items-center h-100">

                                <div>
                                    <img src="@ViewBag.QrCodeUri" alt="QR Code" />
                                </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

    </main><!-- End #main -->


</form>
